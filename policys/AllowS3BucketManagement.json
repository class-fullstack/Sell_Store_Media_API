{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowS3BucketManagement",
      "Effect": "Allow",
      "Action": [
        "s3:ListAllMyBuckets",
        "s3:ListBucket",
        "s3:CreateBucket",
        "s3:DeleteBucket",
        "s3:ForceDeleteBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": ["arn:aws:s3:::*"]
    }
  ]
}
