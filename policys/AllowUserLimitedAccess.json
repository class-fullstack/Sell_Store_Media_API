{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowUserLimitedAccess",
      "Effect": "Allow",
      "Action": [
        "s3:AbortMultipartUpload",
        "s3:ListBucket",
        "s3:ListMultipartUploadParts",
        "s3:PutObject"
      ],
      "Resource": ["arn:aws:s3:::*"]
    }
  ]
}
